    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Spiffy\\DoctrineORMPackage\\": [
                "src/",
                "test/"
